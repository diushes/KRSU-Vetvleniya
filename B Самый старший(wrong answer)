inp=[int(a) for a in input().split(' ')]
largestnum=max(inp)
if inp[0]==inp[1] and inp[1]==inp[2]:
  print('Same age')
elif inp[0]>inp[2] and inp[1]>inp[2] or inp[1]>inp[0] and inp[2]>inp[0] or inp[0]>inp[1] and inp[2]>inp[1]:
  if inp[0]==min(inp):
    print('Anton')
  elif inp[1]==min(inp):
    print('Boris')
  else:
    print('Victor')
elif inp[0]==largestnum:
  print('Anton')
elif inp[1]==largestnum:
  print('Boris')
else:
  print('Victor')
